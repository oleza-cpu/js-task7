    <script type="text/javascript">
  
let model = "Toyota";
let speed = 100;
let car = {
    model,
    speed,
    run() {
        console.log(`${this.model} їде зі швидкістю ${this.speed}`);
    },
    stop() {
        console.log(`${this.model} зупинилася`);
    }
};


car.run();
car.stop();


        </script>
