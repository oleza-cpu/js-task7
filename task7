    <script type="text/javascript">
  
  function calculateAverageSalary(salaries) {
    let total = 0;
    let count = 0;

    for (let person in salaries) {
        if (typeof salaries[person] === 'number' && !isNaN(salaries[person])) {
            total += salaries[person];
            count++;
        }
    }

    if (count === 0) {
        return "No salaries found";
    }

    return total / count;
}


console.log(calculateAverageSalary(salaries1)); 
console.log(calculateAverageSalary(salaries2)); 


        </script>
